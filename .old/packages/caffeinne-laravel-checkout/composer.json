{
    "name": "caffeinne/laravel-checkout",
    "description": "Package containing Laravel adaptation for caffeinne/checkout logic",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0",
        "caffeinne/checkout": "dev-main"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "pestphp/pest": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Caffeinne\\Laravel\\Checkout\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Caffeinne\\Laravel\\Checkout\\Test\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Caffeinne\\Laravel\\Checkout\\ServiceProvider"
            ]
        }
    }
}
