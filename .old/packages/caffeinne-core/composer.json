{
    "name": "caffeinne/core",
    "description": "Package containing the Core domain",
    "license": "MIT",
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "pestphp/pest": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Caffeinne\\Core\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Caffeinne\\Core\\Test\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    }
}
