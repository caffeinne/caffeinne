{
    "name": "caffeinne/checkout",
    "description": "Package containing the Checkout domain logic",
    "license": "MIT",
    "repositories": [
        {
            "type": "path",
            "url": "../*",
            "options": {
                "symlink": true
            }
        }
    ],
    "require": {
        "php": "^8.0",
        "caffeinne/core": "dev-main",
        "caffeinne/catalog": "dev-main",
        "caffeinne/cart": "dev-main",
        "caffeinne/sales": "dev-main"
    },
    "require-dev": {
        "caffeinne/test-suite": "dev-main",
        "friendsofphp/php-cs-fixer": "^3.0",
        "pestphp/pest": "^1.3",
        "symfony/var-dumper": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Caffeinne\\Checkout\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Caffeinne\\Checkout\\Test\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    }
}
