{
    "name": "caffeinne/laravel-sales",
    "description": "Package containing Laravel adaptation for caffeinne/sales logic",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0",
        "caffeinne/sales": "dev-main"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "pestphp/pest": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Caffeinne\\Laravel\\Sales\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Caffeinne\\Laravel\\Sales\\Test\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Caffeinne\\Laravel\\Sales\\ServiceProvider"
            ]
        }
    }
}
