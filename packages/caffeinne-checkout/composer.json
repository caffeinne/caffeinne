{
    "name": "caffeinne/checkout",
    "description": "Package containing the Checkout domain logic",
    "license": "MIT",
    "repositories": [
        {
            "type": "path",
            "url": "../*",
            "options": {
                "symlink": true
            }
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "pestphp/pest": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Caffeinne\\Checkout\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Caffeinne\\Checkout\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Caffeinne\\Checkout\\App\\Providers\\AppServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
