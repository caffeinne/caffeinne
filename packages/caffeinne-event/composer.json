{
    "name": "caffeinne/event",
    "description": "Package containing the Event domain logic",
    "license": "MIT",
    "repositories": [
        {
            "type": "path",
            "url": "../*",
            "options": {
                "symlink": true
            }
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "pestphp/pest": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Caffeinne\\Event\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Caffeinne\\Event\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Caffeinne\\Event\\App\\Providers\\AppServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
