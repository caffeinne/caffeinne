{
    "name": "caffeinne/sales",
    "description": "Package containing the Sales domain",
    "license": "MIT",
    "repositories": [
        {
            "type": "path",
            "url": "../*",
            "options": {
                "symlink": true
            }
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "pestphp/pest": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Caffeinne\\Sales\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Caffeinne\\Sales\\Test\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    }
}
